{
  "name": "@meeg/meeg-blog",
  "version": "0.0.1",
  "private": true,
  "description": "Chris Meagher's blog",
  "author": "Chris Meagher",
  "license": "MIT",
  "scripts": {
    "astro": "astro",
    "build": "pnpm check && astro build",
    "build:dev": "pnpm check && astro build --mode 'build'",
    "check": "pnpm check:ts && pnpm check:astro",
    "check:astro": "astro check",
    "check:ts": "tsc --noEmit",
    "dev": "astro dev",
    "format": "prettier -w --plugin-search-dir=. .",
    "lint": "pnpm lint:js & pnpm lint:css",
    "lint:css": "npx stylelint \"**/*.{css,scss}\" --aei",
    "lint:js": "eslint --ext .js,.ts,.astro src",
    "mkcert:local": "mkcert -install && mkcert localhost",
    "preview": "astro preview",
    "sb:gen": "ts-node \"src/features/storyblok/gen/types.ts\"",
    "sb:login": "storyblok login",
    "sb:logout": "storyblok logout",
    "svg:gen": "svgr --icon --typescript --jsx-runtime automatic --out-dir src/svg -- src/svg",
    "start": "node ./server/production.mjs",
    "start:dev": "cross-env NODE_ENV=production node ./server/development.mjs"
  },
  "dependencies": {
    "@acab/reset.css": "^0.5.3",
    "@astrojs/node": "^3.1.0",
    "@astrojs/preact": "^1.2.0",
    "@fontsource/poppins": "^4.5.10",
    "@sentry/node": "^7.22.0",
    "@storyblok/js": "^2.0.0",
    "express": "^4.18.2",
    "express-urlrewrite": "^2.0.0",
    "helmet": "^6.0.1",
    "is-absolute-url": "^4.0.1",
    "js-cookie": "^3.0.1",
    "open-props": "^1.4.25",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1",
    "pino-sentry": "^0.13.0",
    "postcss-url": "^10.1.3",
    "preact": "^10.11.3",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "shiki": "^0.11.1",
    "sitemap": "^7.1.1",
    "storyblok-rich-text-react-renderer": "^2.6.1"
  },
  "devDependencies": {
    "@svgr/cli": "^6.5.1",
    "@types/fs-extra": "^9.0.13",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^16.18.2",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "astro": "^1.6.12",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-astro": "^0.21.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^11.1.0",
    "pkg-dir": "^7.0.0",
    "postcss": "^8.4.31",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-load-config": "^4.0.1",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.7.0",
    "sass": "^1.56.1",
    "storyblok": "^3.15.0",
    "storyblok-generate-ts": "^1.10.2",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "pnpm": {
    "overrides": {
      "postcss-load-config": "^4.0.1",
      "react": "npm:@preact/compat@latest",
      "react-dom": "npm:@preact/compat@latest"
    }
  }
}
